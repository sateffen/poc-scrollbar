{
  "name": "poc-scrollbar",
  "version": "1.2.0",
  "description": "A simple scrollbar, that just works",
  "homepage": "https://github.com/sateffen/poc-scrollbar",
  "bugs": "https://github.com/sateffen/poc-scrollbar/issues",
  "main": "dist/pocscrollbar.js",
