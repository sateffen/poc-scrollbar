  "devDependencies": {
    "eslint": "8.42.0",
    "eslint-config-google": "0.14.0",
    "jasmine-core": "5.0.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-jasmine": "5.1.0",
    "karma-rollup-preprocessor": "7.0.8",
    "rollup": "3.24.0"
  }
}
