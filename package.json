    "eslint-config-google": "~0.14.0",
    "jasmine-core": "~4.0.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-jasmine": "~4.0.1",
    "karma-rollup-preprocessor": "~7.0.8",
    "rollup": "~2.68.0"
