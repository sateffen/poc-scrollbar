    "eslint-config-google": "~0.9.1",
    "jasmine-core": "~2.8.0",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-jasmine": "~1.1.1",
    "karma-rollup-preprocessor": "~5.0.3",
    "rollup": "~0.53.3",
    "rollup-plugin-buble": "~0.18.0",
    "rollup-watch": "~4.3.1"
