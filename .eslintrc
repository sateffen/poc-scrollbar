{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "jasmine": true
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "rules": {
    "max-len": "off",
    "no-console": "warn",
    "brace-style": [
      "error",
      "stroustrup"
    ]
  }
}